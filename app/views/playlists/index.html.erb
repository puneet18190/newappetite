<h1>Playlist</h1>

<table>
   <th>Playlist<th>
  <%- unless @playlists.nil? %>
  <%- @playlists.each do |obj| %>
   <tr>
      <td>
        <%= link_to obj.name, playlist_tracks_path(obj.id)%>
      </td>
   </tr>
  <%- end %>
  <%- end %>
</table>

<%= link_to 'New Playlist', new_playlist_path(params[:id]) %>